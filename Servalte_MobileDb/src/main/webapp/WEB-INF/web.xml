<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>Adding</servlet-name>
  <servlet-class>com.jsp.servlate.mobiledb.AddMobileServalte</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>Find</servlet-name>
  <servlet-class>com.jsp.servlate.mobiledb.FindMobileServalte</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>Dlt</servlet-name>
  <servlet-class>com.jsp.servlate.mobiledb.DeleteMobileServalte</servlet-class>
  </servlet>
  

  <servlet>
  <servlet-name>up</servlet-name>
  <servlet-class>com.jsp.servlate.mobiledb.UpdateMobileServalte</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Adding</servlet-name>
  <url-pattern>/add-mobile</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>Find</servlet-name>
  <url-pattern>/find-mobile</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>Dlt</servlet-name>
  <url-pattern>/dlt-mobile</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>up</servlet-name>
  <url-pattern>/update-mobile</url-pattern>
  </servlet-mapping>
  
</web-app>
