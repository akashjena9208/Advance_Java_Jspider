<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <!--SERVLATE -->
  <servlet>
  <servlet-name>adding</servlet-name>
  <servlet-class>javax.servalet.ServalteResponse.AddNumberServalteJava</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>sub</servlet-name>
  <servlet-class>javax.servalet.ServalteResponse.SubNumberServalteJava</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>mul</servlet-name>
  <servlet-class>javax.servalet.ServalteResponse.MulNumberServalteJava</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>div</servlet-name>
  <servlet-class>javax.servalet.ServalteResponse.DivNumberServalteJava</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>mod</servlet-name>
  <servlet-class>/javax.servalet.ServalteResponse.ModNumberServalteJava</servlet-class>
  </servlet>
  
  
    <!--SERVLATE  MAPPING-->
  <servlet-mapping>
  <servlet-name>adding</servlet-name>
  <url-pattern>/addTheNumber</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>sub</servlet-name>
  <url-pattern>/subTheNumber</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>mul</servlet-name>
  <url-pattern>/mulTheNumber</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>div</servlet-name>
  <url-pattern>/divTheNumber</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>mod</servlet-name>
  <url-pattern>/modTheNumber</url-pattern>
  </servlet-mapping>
  
</web-app>
